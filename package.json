{
  "name": "dcp-ember-metrics",
  "version": "1.0.0",
  "description": "Based upon https://github.com/adopted-ember-addons/ember-metrics. Send data to multiple analytics integrations without re-implementing new API",
  "keywords": [
    "ember-addon",
    "metrics",
    "analytics",
    "segment",
    "tracking",
    "google analytics",
    "google tag manager",
    "mixpanel",
    "piwik",
    "intercom",
    "facebook pixel",
    "amplitude",
    "azure application insights",
    "pendo"
  ],
  "homepage": "https://github.com/dhochbaum-dcp/dcp-ember-metrics",
  "bugs": "https://github.com/dhochbaum-dcp/dcp-ember-metrics/issues",
  "repository": "https://github.com/dhochbaum-dcp/dcp-ember-metrics",
  "license": "MIT",
  "author": "Lauren Tan",
  "directories": {
    "doc": "./doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "doc": "typedoc",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "test:all": "ember try:each",
    "contributors": "npx contributor-faces -e \"(*-bot|*\\[bot\\]|*-tomster|homu|bors)\"",
    "prepack": "ember ts:precompile",
    "postpack": "ember ts:clean",
    "prepare": "husky install"
  },
  "dependencies": {
    "broccoli-funnel": "^3.0.2",
    "ember-cli-babel": "^7.26.6",
    "ember-cli-typescript": "^5.1.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.4.2",
    "@embroider/test-setup": "^2.0.0",
    "@glimmer/tracking": "^1.0.4",
    "@types/ember-qunit": "^5.0.1",
    "@types/ember-resolver": "^5.0.10",
    "@types/ember__application": "^3.16.3",
    "@types/ember__array": "^4.0.2",
    "@types/ember__component": "^4.0.10",
    "@types/ember__controller": "^4.0.1",
    "@types/ember__debug": "^4.0.1",
    "@types/ember__engine": "^4.0.2",
    "@types/ember__error": "^4.0.0",
    "@types/ember__object": "^4.0.4",
    "@types/ember__polyfills": "^4.0.0",
    "@types/ember__routing": "^4.0.10",
    "@types/ember__runloop": "^4.0.1",
    "@types/ember__service": "^4.0.0",
    "@types/ember__string": "^3.16.3",
    "@types/ember__template": "^4.0.0",
    "@types/ember__test": "^4.0.0",
    "@types/ember__test-helpers": "^2.0.2",
    "@types/ember__utils": "^4.0.0",
    "@types/htmlbars-inline-precompile": "^3.0.0",
    "@types/qunit": "^2.11.2",
    "@types/rsvp": "^4.0.4",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^2.2.0",
    "ember-cli": "~3.28.1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-htmlbars": "^6.0.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-cli-typescript-blueprints": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-qunit": "^6.0.0",
    "ember-resolver": "^8.0.3",
    "ember-sinon": "^5.0.0",
    "ember-sinon-qunit": "^6.0.0",
    "ember-source": "~3.28.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-try": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^11.0.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-qunit": "^7.0.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "qunit": "^2.16.0",
    "qunit-sinon-assertions": "^1.0.0",
    "release-it": "^15.4.1",
    "release-it-lerna-changelog": "^5.0.0",
    "typedoc": "^0.23.14",
    "typescript": "^4.4.3",
    "webpack": "5"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  }
}
