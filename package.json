{
  "name": "ember-metrics",
  "version": "2.0.0-beta.1",
  "description": "Send data to multiple analytics integrations without re-implementing new API",
  "keywords": [
    "ember-addon",
    "metrics",
    "analytics",
    "segment",
    "tracking",
    "google analytics",
    "google tag manager",
    "mixpanel",
    "piwik",
    "intercom",
    "facebook pixel",
    "amplitude",
    "azure application insights",
    "pendo"
  ],
  "homepage": "https://github.com/adopted-ember-addons/ember-metrics",
  "bugs": "https://github.com/adopted-ember-addons/ember-metrics/issues",
  "repository": "https://github.com/adopted-ember-addons/ember-metrics",
  "license": "MIT",
  "author": "Lauren Tan",
  "directories": {
    "doc": "./doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "doc": "typedoc",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "test:all": "ember try:each",
    "contributors": "npx contributor-faces -e \"(*-bot|*\\[bot\\]|*-tomster|homu|bors)\"",
    "prepack": "ember ts:precompile",
    "postpack": "ember ts:clean",
    "prepare": "husky install"
  },
  "dependencies": {
    "broccoli-funnel": "^3.0.2",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-typescript": "^5.1.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.21.3",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.0.1",
    "@ember/test-helpers": "^2.9.3",
    "@embroider/test-setup": "^2.1.1",
    "@glimmer/tracking": "^1.1.2",
    "@types/ember-qunit": "^5.0.1",
    "@types/ember-resolver": "^5.0.10",
    "@types/ember__application": "^3.16.3",
    "@types/ember__array": "^4.0.2",
    "@types/ember__component": "^4.0.10",
    "@types/ember__controller": "^4.0.1",
    "@types/ember__debug": "^4.0.1",
    "@types/ember__engine": "^4.0.2",
    "@types/ember__error": "^4.0.0",
    "@types/ember__object": "^4.0.4",
    "@types/ember__polyfills": "^4.0.0",
    "@types/ember__routing": "^4.0.10",
    "@types/ember__runloop": "^4.0.1",
    "@types/ember__service": "^4.0.0",
    "@types/ember__string": "^3.16.3",
    "@types/ember__template": "^4.0.0",
    "@types/ember__test": "^4.0.0",
    "@types/ember__test-helpers": "^2.0.2",
    "@types/ember__utils": "^4.0.0",
    "@types/htmlbars-inline-precompile": "^3.0.0",
    "@types/qunit": "^2.11.2",
    "@types/rsvp": "^4.0.4",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^8.0.1",
    "ember-auto-import": "^2.6.3",
    "ember-cli": "~4.12.3",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-htmlbars": "^6.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-cli-typescript-blueprints": "^3.0.0",
    "ember-load-initializers": "^2.1.2",
    "ember-qunit": "^6.2.0",
    "ember-resolver": "^10.0.0",
    "ember-sinon": "^5.0.0",
    "ember-sinon-qunit": "^6.0.0",
    "ember-source": "~4.12.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-try": "^2.0.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-ember": "^11.5.0",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^7.3.4",
    "husky": ">=6",
    "lint-staged": ">=10",
    "loader.js": "^4.7.0",
    "prettier": "^2.8.7",
    "qunit": "^2.19.4",
    "qunit-sinon-assertions": "^1.0.0",
    "release-it": "^15.4.1",
    "release-it-lerna-changelog": "^5.0.0",
    "stylelint": "^15.4.0",
    "stylelint-config-standard": "^32.0.0",
    "stylelint-prettier": "^3.0.0",
    "typedoc": "^0.23.14",
    "typescript": "^4.4.3",
    "webpack": "^5.78.0"
  },
  "resolutions": {
    "@babel/plugin-transform-modules-amd": "7.16.5"
  },
  "peerDependencies": {
    "ember-source": ">= 3.28.0"
  },
  "engines": {
    "node": "18.* || 20.* || >= 22"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  }
}
